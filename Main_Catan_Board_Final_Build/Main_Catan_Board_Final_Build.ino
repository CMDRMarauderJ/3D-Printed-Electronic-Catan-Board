        leds[plrLEDs[i]] = CRGB::Black;
        FastLED.show();
        delay(tt);
      }
    }
    i=0;
  }
  i = r;
  int a = 0;
  for (i; i<7; i++)  {                //  Loop with slow down multiplyer
    if (i == 6) {
      i=0;
    }
    if (players[i] == 1) {
      leds[plrLEDs[i]] = CRGB::Green;
      tone(buzzer,NOTE_G6,35);
      FastLED.show();
      delay(tt);
      leds[plrLEDs[i]] = CRGB::Black;
      FastLED.show();
      delay(30);
      tt *= 1.15;       //slow down time multiplier
      if (tt > 1500) {
//        a = i;
        break;
      }
    }
  }
  for (a; a < 3; a++) {
    delay(200);
    leds[plrLEDs[i]] = CRGB::Green;
    tone(buzzer,NOTE_D8,250);
    FastLED.show();
    delay(500);
    if (a < 2){
      leds[plrLEDs[i]] = CRGB::Black;
      FastLED.show();
      delay(250);   
    }
  }
//  Serial.print("s1 = ");
//  Serial.println(i);
  settler1 = i;
  int s2 = 0;
  for (int i = settler1; i < 6; i++) {                                  //Define Settler2 Player
    s2 += players[i];
    if (s2 == 4) {
      settler2 = i;
      break;
    }
    if (i == 5) {
      i = -1;
    }
  }
  display.stopscroll();
  if (lrgGame == true) {
    s2tradeLock = true;
  }
  delay(1000);
  ready2Roll();
//  Serial.println("plrSelect - End");
}

