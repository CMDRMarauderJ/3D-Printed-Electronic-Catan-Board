//------------------------------------------------------setTile-----------------------------------------------------
void setTile (int tile, CRGB color, boolean save){                    //define tile colour
  int currentLED = (tile - 1) * tileSize;
  int lastLED = currentLED + tileSize;
  if (lrgGame == false) {                                           //2-4 Player Board
    for (currentLED; currentLED < lastLED; currentLED++) {   
      leds [smlCurrentTileLEDs[currentLED]] = color;
    }
    if (save){
      smlCurrentTileColors[tile-1] = color;                         //save tile colour
      if (color = all[5]) {                                         //if tile colour black, define desert tile number
        desertTile1 = tile;
      }
    }
  }
  else {                                                          //5-6 Player Board
    for (currentLED; currentLED < lastLED; currentLED++) { 
      leds [lrgCurrentTileLEDs[currentLED]] = color;
    }
    if (save){
      lrgCurrentTileColors[tile-1] = color;                         //save tile colour
      if (color = all[5]) {                                         //if tile colour black, define deserts tile number
        int i;
        i ++;
        if (i = 1) {
          desertTile1 = tile;
        }
        else {
          desertTile2 = tile;
        }
      }
    }
  }
}
//-----------------------------------------------------setPort------------------------------------------------------
void setPort (int port, CRGB color, boolean save){
  int LED = (port - 1);
  if (lrgGame == false) {
    leds [pgm_read_byte(smlPortLEDs+LED)] = color;
    if (save){
      smlCurrentPortColors[port-1] = color;
    }
  }
  else {
    leds [pgm_read_byte(lrgPortLEDs+LED)] = color;
    if (save){
      lrgCurrentPortColors[port-1] = color;
    }
  }
}

//------------------------------------------------------beep-----------------------------------------------------
void beep(){
  tone(buzzer,NOTE_G4,35);
  delay(35);
  tone(buzzer,NOTE_G5,35);
  delay(35);
  tone(buzzer,NOTE_G6,35);
  delay(35);
  noTone(buzzer);
}

//------------------------------------------------------buildUp-----------------------------------------------------

void buildUp() {                                                        //rolling dice graphic
  for (int i=0; i<44; i++) {
    display.clearDisplay();
    display.drawBitmap(12, 17,dice[i],104,48, 1);
    display.display();
    delay(20);
    
  }
  delay(500);
}

