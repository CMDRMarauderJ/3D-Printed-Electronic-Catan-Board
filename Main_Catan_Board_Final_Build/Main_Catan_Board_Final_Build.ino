  display.display();
  int t=0;
  int buttPress;
  unsigned long previousMillis = 0;
  
  while (t < 3) {
    const int interval = 1000;
    for (int i=0; i<6; i++) {
      buttonArray[i].loop();
      buttPress = buttonArray[i].getState();
      if (buttPress = LOW) {
        beep();
      }
      while (buttPress == LOW) {
        unsigned long currentMillis = millis();
        if (currentMillis - previousMillis >= interval) {
          previousMillis = currentMillis;
          (t++);
        }
        if (t == 3) {
