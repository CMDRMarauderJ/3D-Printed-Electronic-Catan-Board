//-----------------------------------------------------startGame------------------------------------------------------
void startGame() {
  delay(2000);
  int x = 128;
  for (int i = 0; i<33; i++) {
    display.clearDisplay();
    display.drawBitmap(x, 0, The_Settlers_Of, 128, 11, 1);
    display.drawBitmap(-x, 16, catan_logo, 128, 47, 1);
    display.display();
    delay(5);
    x -= 4;
  }
  for (int thisNote = 0; thisNote < notes * 2; thisNote = thisNote + 2) {
    divider = songofstorms[thisNote + 1];
    if (divider > 0) {
      noteDuration = (wholenote) / divider;
    }
    else if (divider < 0) {
      noteDuration = (wholenote) / abs(divider);
      noteDuration *= 1.5;
    }
    tone(buzzer, songofstorms[thisNote], noteDuration*0.9);
    delay(noteDuration);
    noTone(buzzer);
  }
  delay(2000);
  plrSelect();
}

