        if (g == i) {
          lrgCurrentTileColors[j] = all[i];
        }
      }
      for (int j=0; j<lrgPortRange; j++) {
        int g = 0;
        g = EEPROM.read(j+31);                            //Eeprom 31 - 41 = lrgGame ports colours
        if (g == i) {
          lrgCurrentPortColors[j] = all[i];
        }
      }
    }
    for (int i = 0; i < lrgTileRange; i++) {
      setTile(i+1, lrgCurrentTileColors[i], true);
      FastLED.show();
    }
    for (int i = 0; i < lrgPortRange; i++) {   
      setPort(i+1, lrgCurrentPortColors[i], true);
      FastLED.show();
